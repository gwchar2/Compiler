   /* should print 3  8   16.0   17.0  18.0 (or 19.0 if RTOI rounds up) */    
a,b : float;
i, j : int;

{
   a = 3.2;
   j = 8;
   
   i = cast<int>(a);
   output(i);
   
   i = cast<int>(j);
   output(i);
   
   i = 16;
   a = cast<float>(i);
   output(a);
   
   b = 17.0;
   
   a = cast<float>(b);
   
   output(a);
   
   b = 18.9;
   a = cast<int> (b);
   
   output(a);

}
